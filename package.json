    "babel-preset-stage-2": "^6.22.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.2",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "jshint": "^2.9.4",
    "jshint-loader": "^0.8.4",
    "mocha": "^3.2.0",
    "mocha-webpack": "next",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-merge": "^3.0.0",
    "webpack-node-externals": "^1.5.4"
  },
  "dependencies": {
    "hack": "^0.7.7",
    "lodash": "^4.17.4",
