{
  "name": "store-aware-ractive",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run dev",
    "watch": "cross-env NODE_ENV=development webpack --config webpack.config.js --watch --progress",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js --profile --inline --progress --hot",
    "build": "cross-env NODE_ENV=production webpack --config web/webpack.config.js -p",
    "test": "cross-env NODE_ENV=test mocha-webpack --opts test/conf/mocha-webpack.opts"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
